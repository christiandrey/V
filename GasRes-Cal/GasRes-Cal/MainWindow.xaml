                                    <Label.Triggers>
                                        <EventTrigger RoutedEvent="UIElement.MouseEnter">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <ColorAnimation Storyboard.TargetName="mb_btn"
														Storyboard.TargetProperty="(Control.Background).(SolidColorBrush.Color)"
														From="#00F5F5F5"
														To="#FFE0E0E0"
														Duration="0:0:0.3"
                                                                    />
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                        <EventTrigger RoutedEvent="UIElement.MouseLeave">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <ColorAnimation Storyboard.TargetName="mb_btn"
