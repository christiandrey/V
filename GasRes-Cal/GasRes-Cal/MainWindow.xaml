                                    <Label.Triggers>
                                        <EventTrigger RoutedEvent="UIElement.MouseEnter">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <ColorAnimation Storyboard.TargetName="wwi_btn"
														Storyboard.TargetProperty="(Control.Background).(SolidColorBrush.Color)"
														From="#00F5F5F5"
														To="#FFE0E0E0"
														Duration="0:0:0.3"
                                                                    />
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                        <EventTrigger RoutedEvent="UIElement.MouseLeave">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <ColorAnimation Storyboard.TargetName="wwi_btn"
														Storyboard.TargetProperty="(Control.Background).(SolidColorBrush.Color)"
														From="#ffe0e0e0"
														To="#00F5F5F5"
														Duration="0:0:0.3"
                                                                    />
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                    </Label.Triggers>
                                </Label>
                                <Label x:Name="wow_btn" Content="Without water influx" Grid.Column="2" FontFamily="Segoe UI Semibold" Foreground="#FF616161" VerticalContentAlignment="Center" Padding="40,0,0,0" Grid.Row="2" Cursor="Hand" Margin="0,30,0,80" Height="30" Background="#00F5F5F5" Visibility="Hidden">
                                    <Label.Triggers>
                                        <EventTrigger RoutedEvent="UIElement.MouseEnter">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <ColorAnimation Storyboard.TargetName="wow_btn"
														Storyboard.TargetProperty="(Control.Background).(SolidColorBrush.Color)"
														From="#00F5F5F5"
														To="#FFE0E0E0"
														Duration="0:0:0.3"
                                                                    />
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                        <EventTrigger RoutedEvent="UIElement.MouseLeave">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <ColorAnimation Storyboard.TargetName="wow_btn"
														Storyboard.TargetProperty="(Control.Background).(SolidColorBrush.Color)"
														From="#ffe0e0e0"
														To="#00F5F5F5"
														Duration="0:0:0.3"
                                                                    />
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                    </Label.Triggers>
                                </Label>
                            </Grid>
                            <Label x:Name="begincalc" Content="Begin Calculation" Grid.Column="1" HorizontalAlignment="Center" Margin="49,94,50,94" VerticalAlignment="Center" Width="126" Height="32" FontFamily="Segoe WP Semibold" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF629FF7" Foreground="White" Cursor="Hand">
                                <Label.Triggers>
                                    <EventTrigger RoutedEvent="UIElement.MouseEnter">
                                        <BeginStoryboard>
                                            <Storyboard>
                                                <ColorAnimation Storyboard.TargetName="begincalc"
													Storyboard.TargetProperty="(Control.Background).(SolidColorBrush.Color)"
													From="#FF629FF7"
													To="#FF457CCD"
													Duration="0:0:0.3"
                                                                    />
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>
                                    <EventTrigger RoutedEvent="UIElement.MouseLeave">
                                        <BeginStoryboard>
                                            <Storyboard>
                                                <ColorAnimation Storyboard.TargetName="begincalc"
													Storyboard.TargetProperty="(Control.Background).(SolidColorBrush.Color)"
													From="#457CCD"
													To="#FF629FF7"
													Duration="0:0:0.3"
                                                                    />
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>
                                </Label.Triggers>
                            </Label>
                            <Rectangle Grid.Column="1" HorizontalAlignment="Left" Margin="0" Stroke="#FFCFCFCF" Width="1"/>
                        </Grid>
                    </Grid>
                    <Label x:Name="errorlabel" Content="Error: Please select a task!!!" Margin="0" VerticalAlignment="Bottom" Height="30" Background="#FFEA3838" Foreground="White" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Visibility="Hidden"/>
                </Grid>
                <Rectangle Margin="0" Grid.Row="2" Fill="White" Stroke="#FFCFCFCF" VerticalAlignment="Top" Height="1"/>
                <Label Content="© Umeokeke Chiemelie Paul, 158334, 2015" Margin="0" Grid.Row="2" Padding="20,0,0,0" VerticalContentAlignment="Center"/>
            </Grid>
        </Grid>
        <Grid x:Name="softgrid" Margin="0">
            <Grid.RowDefinitions>
                <RowDefinition Height="100"/>
                <RowDefinition Height="44"/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Grid x:Name="softgridheader" Margin="0">
