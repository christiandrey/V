                        <EventTrigger RoutedEvent="UIElement.MouseEnter">
                            <BeginStoryboard>
                                <Storyboard>
                                    <ColorAnimation Storyboard.TargetName="homebtn"
													Storyboard.TargetProperty="(Control.Background).(SolidColorBrush.Color)"
													From="#FF629FF7"
													To="#FF457CCD"
													Duration="0:0:0.3"
                                                                    />
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                        <EventTrigger RoutedEvent="UIElement.MouseLeave">
                            <BeginStoryboard>
                                <Storyboard>
                                    <ColorAnimation Storyboard.TargetName="homebtn"
													Storyboard.TargetProperty="(Control.Background).(SolidColorBrush.Color)"
