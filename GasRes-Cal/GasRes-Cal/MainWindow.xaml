    <Window.Resources>
        <Storyboard x:Key="blinkanim" x:Name="blinkstory">
            <ColorAnimation Storyboard.TargetName="errorlabel"
                            Storyboard.TargetProperty="(Label.Background).(SolidColorBrush.Color)"
                            From="#FFEA3838"
                            To="#FFF5F5F5"
                            Duration="0:0:0.1"
                            AutoReverse="True"
                            RepeatBehavior="3x">
            </ColorAnimation>
        </Storyboard>
        <Storyboard x:Key="blinkanim2" x:Name="blinkstory2">
            <ColorAnimation Storyboard.TargetName="footererrorlabel"
                            Storyboard.TargetProperty="(Label.Background).(SolidColorBrush.Color)"
                            From="#FFEA3838"
                            To="#FFF5F5F5"
                            Duration="0:0:0.1"
                            AutoReverse="True"
                            RepeatBehavior="3x">
            </ColorAnimation>
        </Storyboard>
        <Storyboard x:Key="blinkanim3" x:Name="blinkstory3">
            <ColorAnimation Storyboard.TargetName="statuslabel"
                            Storyboard.TargetProperty="(Label.Background).(SolidColorBrush.Color)"
                            From="#FF404040"
                            To="#FFF5F5F5"
                            Duration="0:0:0.1"
                            AutoReverse="True"
                            RepeatBehavior="3x">
            </ColorAnimation>
        </Storyboard>
    </Window.Resources>
