                                        <EventTrigger RoutedEvent="UIElement.MouseEnter">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <ColorAnimation Storyboard.TargetName="vm_btn"
														Storyboard.TargetProperty="(Control.Background).(SolidColorBrush.Color)"
														From="#00F5F5F5"
														To="#FFE0E0E0"
														Duration="0:0:0.3"
                                                                    />
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                        <EventTrigger RoutedEvent="UIElement.MouseLeave">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <ColorAnimation Storyboard.TargetName="vm_btn"
														Storyboard.TargetProperty="(Control.Background).(SolidColorBrush.Color)"
														From="#ffe0e0e0"
														To="#00F5F5F5"
														Duration="0:0:0.3"
                                                                    />
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                    </Label.Triggers>
                                </Label>
                                <Image x:Name="volimg_Copy" Grid.Column="1" Grid.Row="1" Source="Mb.png"/>
